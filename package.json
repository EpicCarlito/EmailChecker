{
  "name": "@epiccarlito/emailchecker",
  "version": "1.1.3",
  "description": "A simple way to check if an email exists",
  "keywords": [
    "typescript",
    "email",
    "check",
    "verify",
    "validate"
  ],
  "type": "module",
  "exports": {
    "require": {
      "types": "./build/cjs/index.d.ts",
      "default": "./build/cjs/index.js"
    },
    "import": {
      "types": "./build/esm/index.d.ts",
      "default": "./build/esm/index.js"
    }
  },
  "main": "./build/esm/index.js",
  "scripts": {
    "build": "tsc --module commonjs --outDir build/cjs && tsc --module es2022 --outDir build/esm"
   },
  "repository": {
    "type": "git",
    "url": "https://github.com/EpicCarlito/EmailChecker.git"
  },
  "author": "EpicCarlito",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EpicCarlito/EmailChecker/issues"
  },
  "dependencies": {
    "@types/node": "^20.4.10"
  },
  "devDependencies": {
    "typescript": "^5.4.3"
  }
}
